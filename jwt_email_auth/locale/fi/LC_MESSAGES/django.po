#: .\jwt_email_auth\exceptions.py:20
msgid "Server did not respond."
msgstr "Palvelin ei vastaa."

#: .\jwt_email_auth\exceptions.py:26
msgid "This user is not allowed to send another login code yet."
msgstr "Tämä käyttäjä ei voi vielä lähettää toista kirjautumiskoodia."

#: .\jwt_email_auth\exceptions.py:32
#, python-format
msgid "Maximum number of attempts reached. Try again in %(x)s minutes."
msgstr ""
"Sallittu määrä kirjautumisyrityksiä ylitetty. Yritä uudeleen %(x)s minuutin "
"päästä."

#: .\jwt_email_auth\exceptions.py:42
#, python-format
msgid "'%(claim)s' not found from the list of expected claims."
msgstr "'%(claim)s' ei löytynyt odotettujen arvojen joukosta."

#: .\jwt_email_auth\exceptions.py:52
#, python-format
msgid "Not allowed to update claim '%(claim)s'."
msgstr "Ei sallittu päivitystä arvolle '%(claim)s'"

#: .\jwt_email_auth\exceptions.py:62
msgid "Data was corrupted."
msgstr "Data korruptoitunut."

#: .\jwt_email_auth\permissions.py:22 .\jwt_email_auth\tokens.py:89
msgid "Invalid token."
msgstr "Virheellinen tokeni."

#: .\jwt_email_auth\rotation\apps.py:7
msgid "JWT rotation"
msgstr "JWT rotaatio"

#: .\jwt_email_auth\tokens.py:81
msgid "Signature has expired."
msgstr "Allekirjoitus on vahnentunut."

#: .\jwt_email_auth\tokens.py:85
msgid "Error decoding signature."
msgstr "Virhe allekirjoituksen tulkitsemisessa."

#: .\jwt_email_auth\tokens.py:93
msgid "Unexpected error."
msgstr "Odottamaton virhe."

#: .\jwt_email_auth\tokens.py:121
msgid "No token found from request cookies."
msgstr "Pyyntöevästeistä ei löytynyt tokenia."

#: .\jwt_email_auth\tokens.py:174
msgid "Invalid token type."
msgstr "Virheellinen tokenin tyyppi."

#: .\jwt_email_auth\tokens.py:224
msgid "Token is no longer accepted."
msgstr "Tokenia ei enää hyväksytä."

#: .\jwt_email_auth\utils.py:130
msgid "No Authorization header found from request."
msgstr "Authorization headeria ei löytynyt."

#: .\jwt_email_auth\utils.py:135
msgid "Invalid Authorization header."
msgstr "Virheellinen Authorization header."

#: .\jwt_email_auth\utils.py:138
msgid "Invalid prefix."
msgstr "Virheellinen etuliite."

#: .\jwt_email_auth\utils.py:148
msgid "JWT decrypt failed."
msgstr "Tokenin salauksen purku epäonnistui"

#: .\jwt_email_auth\utils.py:152
msgid "Invalid JWT format."
msgstr "Virheellinen tokenin muoto."

#: .\jwt_email_auth\utils.py:187 .\jwt_email_auth\utils.py:201
msgid "Cipher key not set."
msgstr "Salausavainta ei ole asetettu."

#: .\jwt_email_auth\utils.py:189 .\jwt_email_auth\utils.py:203
msgid "Invalid cipher key."
msgstr "Virheellinen salausavain."

#: .\jwt_email_auth\utils.py:213
msgid "Wrong cipher key."
msgstr "Väärä salausavain."

#: .\jwt_email_auth\views.py:156
msgid "Failed to send login codes. Try again later."
msgstr "Ei voitu lähettää kirjautumiskoodia. Yritä uudelleen myöhemmin."

#: .\jwt_email_auth\views.py:185
#, python-format
msgid "No login code found for '%(value)s'."
msgstr "Ei löydetty kirjautumiskoodia '%(value)s':lle."

#: .\jwt_email_auth\views.py:190
msgid "Incorrect login code."
msgstr "Väärä kirjautumiskoodi."

#: .\jwt_email_auth\views.py:198
msgid "Data was corrupted. Try to send another login code."
msgstr "Data korruptoitunut. Yritä lähettää uusi kirjautumiskoodi."
